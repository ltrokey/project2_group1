<h1>Welcome, {{username}}!</h1>

<h2>Quest Items</h2>
<div class="productContainer">
  {{#each userProducts as |product|}}
    <div class="productCards">
      <h3>{{product.title}}</h3>
      <div>
        <a href="/product/{{product.id}}">
          <img src="/images/{{product.filename}}" alt="{{product.title}}" />
        </a>
      </div>
    </div>
  {{/each}}
</div>